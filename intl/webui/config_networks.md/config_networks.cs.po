msgid "DVB-C"
msgstr ""

msgid ": Cable TV, delivered via a cable to your house"
msgstr ""

msgid "DVB-S"
msgstr ""

msgid ": Satellite (includes S2), so any signal coming in via a dish"
msgstr ""

msgid "DVB-T"
msgstr ""

msgid ""
": Terrestrial, so over-the-air broadcasts received through a traditional "
"television aerial"
msgstr ""

msgid "ATSC"
msgstr ""

msgid ""
": Over-the-air terrestrial, common in north and central America and parts of "
"south Asia"
msgstr ""

msgid "IPTV"
msgstr ""

msgid ": IP, so over the Internet"
msgstr ""

